{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020", "DOM"],
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": false,
    "sourceMap": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "incremental": true,
    "noEmitOnError": false
  },
  "include": [
    "src/main.ts",
    "src/core/autonomousPostingEngine.ts", 
    "src/ai/socialContentOperator.ts",
    "src/ai/engagementManager.ts",
    "src/intelligence/engagementOptimizer.ts",
    "src/intelligence/engagementMonitor.ts",
    "src/ai/viralGenerator.ts",
    "src/ai/viralPrompts.ts",
    "src/ai/generate.ts",
    "src/ai/prompts.ts",
    "src/ai/threadGenerator.ts",
    "src/lib/supabaseClients.ts",
    "src/agents/viralFollowerGrowthMaster.ts",
    "src/agents/intelligentReplyEngine.ts",
    "src/agents/intelligentCommunityEngagementEngine.ts",
    "src/agents/competitiveIntelligenceEngine.ts",
    "src/intelligence/masterAIIntelligenceCoordinator.ts",
    "src/intelligence/supremeContentIntelligence.ts",
    "src/utils/supabaseClient.ts",
    "src/utils/resilientSupabaseClient.ts",
    "src/utils/emergencyOfflineMode.ts",
    "src/utils/bulletproofTwitterDataCollector.ts",
    "src/utils/offlineDataSynchronizer.ts",
    "src/utils/supremeAIPromptOptimizer.ts",
    "src/utils/systemIntelligenceOptimizer.ts",
    "src/utils/runtimeConfigManager.ts",
    "src/utils/immediateGrowthAccelerator.ts",
    "src/utils/budgetAwareOpenAI.ts",
    "src/utils/emergencyBudgetLockdown.ts",
    "src/utils/openaiClient.ts",
    "src/utils/xClient.ts",
    "src/utils/browserTweetPoster.ts",
    "src/utils/modernTwitterSelectors.ts",
    "src/utils/emergencyBrowserPoster.ts",
    "src/utils/threadUtils.ts",
    "src/utils/intelligentPostTypeDetector.ts",
    "src/utils/railwayResourceMonitor.ts",
    "src/utils/followerGrowthLearner.ts",
    "src/agents/intelligentPostingOptimizerAgent.ts",
    "src/utils/aiService.ts",
    "src/utils/postingLock.ts",
    "src/config/persona.ts",
    "src/jobs/**/*",
    "src/types/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "**/*.test.ts",
    "**/*.spec.ts",
    "tests/**/*"
  ]
}